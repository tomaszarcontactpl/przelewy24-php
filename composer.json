{
    "name": "tomaszarcontactpl/przelewy24-php",
    "description": "Przelewy24 PHP library",
    "keywords": [
        "przelewy24",
        "payments"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/tomaszarcontactpl/przelewy24-php",
    "authors": [
        {
            "name": "Tomasz Majerski",
            "homepage": "https://www.arcontact.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.34",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Przelewy24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Przelewy24\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
